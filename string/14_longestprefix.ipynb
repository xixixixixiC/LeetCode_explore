   "source": [
    "class Solution:\n",
    "    def longestCommonPrefix(self, strs: List[str]) -> str:\n",
    "        prefix=\"\"\n",
    "        letter_tuple=zip(*strs)\n",
    "        for zu in letter_tuple:\n",
    "            if len(set(zu))>1:\n",
    "                break\n",
    "            prefix=prefix+zu[0]\n",
    "        return prefix"
   ]
  }
